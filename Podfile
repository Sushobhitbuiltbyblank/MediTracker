# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

def sharedpods
  use_frameworks!
  
  # ignore all warnings from all pods
  inhibit_all_warnings!
  
  # Pods for MediTracker
  pod 'SwiftLint'
end

target 'MediTracker' do
 sharedpods
end

target 'MediTrackerTests' do
    sharedpods
end

target 'MediTrackerUITests' do
    sharedpods
end

